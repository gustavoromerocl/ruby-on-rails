<div class="large-container center-block">
    <h1>Ultimos publicados</h1>
    <% @last_articles.each do |article| %>
        <div class="card padding margin-top" >
            <h3 class="text-uppercase"> 
                <%= article.title%>
            </h3>
            <p class="text-secondary"><%= article.created_at%></p>
            <% if user_signed_in? %>
                <%= link_to "Ver más", article %>
            <% end %>
        </div>
    <%end%>
    
    <h1>Mis artículos</h1>
    <p>Cantidad de articulos: <%= @articles.count%></p>
    <%= render @articles %>


    <div>
        <%= will_paginate @articles %>
    </div>
</div>


